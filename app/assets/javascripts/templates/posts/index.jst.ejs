<h1>Posts</h1>
<% posts.each(function(post) { %>
<a href="/posts/<%= post.get("id") %>">
	<div class="row post">
		<p><%= post.get("title") %></p>
		<p><%= posts.getCategory(post.get("category")) %></p>
		<p><%= post.get("description") %></p>
		<p><%= post.get("candidate_name") %></p>
		<p><%= post.get("deadline") %></p>
	</div>
</a>
<% }) %>
<a href="/posts/new">Add Post</a>
